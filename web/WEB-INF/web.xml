<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>addblogs</servlet-name>
        <servlet-class>addblogs</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addsub</servlet-name>
        <servlet-class>addsub</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>changepwd</servlet-name>
        <servlet-class>changepwd</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>temp</servlet-name>
        <servlet-class>temp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sendMail</servlet-name>
        <servlet-class>sendMail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>gordleave</servlet-name>
        <servlet-class>gordleave</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>storeTemp</servlet-name>
        <servlet-class>storeTemp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>likeblog</servlet-name>
        <servlet-class>likeblog</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addblogs</servlet-name>
        <url-pattern>/addblogs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addsub</servlet-name>
        <url-pattern>/addsub</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>changepwd</servlet-name>
        <url-pattern>/changepwd</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>temp</servlet-name>
        <url-pattern>/temp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sendMail</servlet-name>
        <url-pattern>/sendMail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>gordleave</servlet-name>
        <url-pattern>/gordleave</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>storeTemp</servlet-name>
        <url-pattern>/storeTemp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>likeblog</servlet-name>
        <url-pattern>/likeblog</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            100
        </session-timeout>
    </session-config>
</web-app>
